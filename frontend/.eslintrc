{
	"env": {
	  "browser": true,
	  "node": true,
	  "jest": true
	},
	"parserOptions": {
	  "ecmaVersion": 2020,
	  "sourceType": "module",
	  "ecmaFeatures": {
		"jsx": true
	  }
	},
	"plugins": [
	  "react"
	],
	"extends": [
	  "eslint:recommended",
	  "plugin:react/recommended"
	],
	"rules": {
	  "no-unused-vars": "off",
	  "react/prop-types": "off"
	}
}
